{
  "name": "@raae/gatsby-remark-oembed",
  "author": "@raae (queen.raae.codes)",
  "version": "0.1.1",
  "license": "MIT",
  "description": "> A GatsbyJS Plugin that transforms oembed links into its corresponding embed code.",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "oembed",
    "remark",
    "youtube",
    "twitter",
    "codepen",
    "vimeo",
    "soundcloud",
    "instagram"
  ],
  "scripts": {
    "develop": "jest --watch",
    "lint": "eslint .",
    "prettier": "prettier \"**/*.+(js|json|yml)\" --ignore-path \"./.prefetched-providers.json\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run lint && npm run prettier  -- --list-different && npm run test",
    "test": "jest --coverage",
    "prepublish": "./prepublish.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "lodash": "^4.17.14",
    "unist-util-select": "^1.5.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.16.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-jest": "^22.5.1",
    "husky": "^2.2.0",
    "jest": "^23.6.0",
    "prettier": "^1.17.0"
  },
  "main": "index.js",
  "homepage": "https://github.com/lillylabs/gatsby-remark-oembed#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lillylabs/gatsby-remark-oembed.git"
  },
  "bugs": {
    "url": "https://github.com/lillylabs/gatsby-remark-oembed/issues"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run validate"
    }
  }
}
